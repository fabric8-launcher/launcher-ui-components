{
  "name": "launcher-frontend",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "packages/launcher-client",
    "packages/keycloak-react",
    "packages/launcher-component",
    "packages/launcher-app"
  ],
  "dependencies": {
    "@patternfly/react-core": "2.1.4",
    "@patternfly/react-icons": "3.0.2",
    "classnames": "2.2.6",
    "react": "16.8.3",
    "react-dom": "16.8.3"
  },
  "scripts": {
    "test": "yarn workspaces run test",
    "build:libs": "yarn workspace launcher-client build && yarn workspace keycloak-react build && yarn workspace launcher-component build",
    "build": "rm -rf build && yarn workspaces run build && cp -r ./packages/launcher-app/build ./build",
    "launcher-app:start": "yarn workspace launcher-app start:staging-api",
    "launcher-app:build": "yarn workspace launcher-app build",
    "launcher-component:test": "yarn workspace launcher-component test",
    "launcher-component:storybook": "yarn workspace launcher-component storybook",
    "launcher-component:storybook:publish": "yarn workspace launcher-component storybook:publish"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/jest": "24.0.6",
    "@types/lodash": "4.14.121",
    "@types/node": "11.9.4",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "eslint": "5.15.1",
    "ghooks": "2.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
    "lerna": "3.13.0",
    "node-sass": "4.11.0",
    "react-testing-library": "6.0.0",
    "rollup": "1.3.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-filesize": "6.0.1",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-tslint": "0.2.2",
    "rollup-plugin-typescript2": "0.19.3",
    "ts-jest": "24.0.0",
    "tslint": "5.13.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3333"
  }
}
